{
    "compilerOptions": {
        "lib": [
            "es2017",
            "dom"
        ],
        "strict": false,
        "strictNullChecks": false,
        "noImplicitThis": true,
        "alwaysStrict": false,
        "outDir": "./dist/",
        "declaration": false,
        "sourceMap": true,
        "target": "es5",
        "module": "commonJS",
        "baseUrl": ".",
        "paths": {
            "*": [
                "./typings/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}